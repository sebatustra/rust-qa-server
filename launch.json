{
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'practical-rust-book'",
      "cargo": {
        "args": [
          "build",
          "--bin=practical-rust-book",
          "--package=practical-rust-book"
        ],
        "filter": {
          "name": "practical-rust-book",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'practical-rust-book'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=practical-rust-book",
          "--package=practical-rust-book"
        ],
        "filter": {
          "name": "practical-rust-book",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
